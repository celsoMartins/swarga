<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <ul class="breadcrumb">
        <li><%= link_to CampingGroup.model_name.human(count: 2), camping_groups_path %></li>
        <li><%= t('camping_groups.new.title') %></li>
      </ul>
    </div>
  </div>

  <%= form_for @camping_group do |f| %>
    <div class="row">
      <div class="col-xs-12">
        <%= f.submit class: 'btn btn-success' %>
      </div>
    </div>

    <div class="row botton-spaced-component">
      <div class="col-xs-12">
        <%= render 'form', f: f %>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12">
        <%= f.submit class: 'btn btn-success' %>
      </div>
    </div>
  <% end %>
</div>
